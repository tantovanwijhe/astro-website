---
import Layout from "../../layouts/BaseLayout.astro"

const allPosts = await Astro.glob("./*.md")
---

<Layout title="Journey">
  <p>My page content, wrapped in a layout!</p>
  <ul>
    {
      allPosts.map(post => (
        <li>
          <a href={post.url}>{post.frontmatter.title}</a>
        </li>
      ))
    }
  </ul>
</Layout>
